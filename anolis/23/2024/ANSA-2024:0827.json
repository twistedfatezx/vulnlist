  "Metadata": {
    "Title": "ANSA-2024:0827: qemu security update (Moderate)",
    "Affected": {
      "Family": "unix",
      "Platform": [
        "Anolis OS 23"
      ]
    },
    "References": [
      {
        "RefID": "ANSA-2024:0827",
