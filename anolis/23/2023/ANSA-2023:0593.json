  "Metadata": {
    "Title": "ANSA-2023:0593: php security update (Important)",
    "Affected": {
      "Family": "unix",
      "Platform": [
        "Anolis OS 23"
      ]
    },
    "References": [
      {
        "RefID": "ANSA-2023:0593",
